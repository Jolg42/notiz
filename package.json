  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "npm run build && npm run scully && npm run scully:serve",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*{.ts,.scss}\"",
    "scully": "scully",
    "scully:build": "npm run build:prod && npm run scully && sh 404.sh",
    "scully:serve": "scully serve",
    "blick:serve": "blick open http://localhost:4200 & ng s",
    "optimize:images": "node optimize-images.js",
    "build:image-template": "ng build image-template --prod --output-hashing none && npm run package:image-template",
    "package:image-template": "cat ./dist/image-template/{runtime,polyfills,main}-es5.js > projects/image-template/dist/image-template.js && ls -lah projects/image-template/dist/image-template.js && cp dist/image-template/styles.css projects/image-template/dist",
    "link:add": "node projects/link-generator && ng build && npm run scully"
  },
